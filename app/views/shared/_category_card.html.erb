<div class="row">
  <div class="col-4">
    <img src="<%= category.icon %>" alt="<%= category.name %>">
  </div>
  <div class="col-8">
    <p><%= category.name %></p>
    <p>$ <%= format('%.2f', category.expenses.sum(:amount)) %></p>
    <p><%= category.updated_at.strftime('%d %b %Y') %></p>
  </div>
</div>